{
  "name": "rech-editor-vscode",
  "version": "0.0.1",
  "description": "Common functions and classes for simplifying the VSCode and Node API usage",
  "keywords": [
    "commons",
    "VSCode"
  ],
  "repository": "https://github.com/RechInformatica/rech-editor-vscode.git",
  "license": "MIT",
  "engines": {
    "vscode": "^1.1.6"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
