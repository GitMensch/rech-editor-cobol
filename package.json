{
  "name": "rech-editor-vscode",
  "version": "0.0.1",
  "publisher": "Rech Informática",
  "description": "Common functions and classes for simplifying the VSCode and Node API usage",
  "main": "index.ts",
  "keywords": [
    "commons",
    "VSCode"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RechInformatica/rech-editor-vscode.git"
  },
