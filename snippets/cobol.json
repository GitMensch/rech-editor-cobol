		"description": "RechDoc de valor default"
	},
	"search" : {
		"prefix": "search",
		"body": [
			"      *>-> Busca ... na tabela de ...",
			"           search all             <Nome_Tabela>",
			"      *>-> Se chegou ao fim da busca",
			"              at end",
			"                 ...",
			"      *>-> Se encontrou",
			"              when <condição>",
			"                 next             sentence,",
			"           end-search."
			],
		"description": "Busca em uma tabela"
	},
